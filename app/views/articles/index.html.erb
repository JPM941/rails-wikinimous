<h1>Wikidiap√©</h1>
<button type="button" class="btn btn-success"><%= link_to "Create",
  "articles/new" %></button>
<div>
  <% @articles.each do |article| %>
    <div class="card">

      <h5 class="card-header"><%=link_to article.title, article_path(article)%></h5>
      <div class="card-body">
          <p class="card-text"><%=article.content%></p>
      </div>
    </div>


  <% end %>
</div>
